<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_marginTop="0dp"
  android:background="@drawable/alert_dialog_bg"
  android:paddingLeft="10dp"
  android:paddingRight="1dp"
  android:paddingTop="20dp"
  android:paddingBottom="20dp"
  android:layout_width="match_parent"
  android:layout_height="match_parent">
  
    
    <TableLayout 
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        
        <TableRow 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
        
            <TextView 
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:text="License No."/>
	   
	    	<TextView 
	    	    android:id="@+id/licenseNo"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content" />
            
        </TableRow>
        
        <TableRow 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
        
            <TextView 
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:text="Date In"/>
	   
	    	<TextView 
	    	    android:id="@+id/dateIn"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content" />
            
        </TableRow>
        
        <TableRow 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
        
            <TextView 
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:text="Date Out" />
	   
	    	<TextView 
	    	    android:id="@+id/dateOut"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content" />
            
        </TableRow>
        
        <TableRow 
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
        
            <TextView 
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:text="Parking Time" />
	   
	    	<TextView 
	    	    android:id="@+id/parkingTime"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content" />
            
        </TableRow>
        
        <TableRow 
            android:layout_marginTop="10dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
	   
	    	<TextView 
	    	    android:id="@+id/price"
		        android:layout_width="match_parent"
		        android:layout_height="wrap_content"
		        android:textStyle="bold"
		        android:textSize="25sp"
		        android:layout_span="2"
		        android:gravity="center" />
            
        </TableRow>
        
    </TableLayout>
    
</LinearLayout>